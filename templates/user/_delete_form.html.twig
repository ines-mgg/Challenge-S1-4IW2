<form
  method="post"
  action="{{ path('app_user_delete', { id: user.id }) }}"
  onsubmit="return confirm('Are you sure you want to Supprimer this item?');"
>
  <input
    type="hidden"
    name="_token"
    value="{{ csrf_token('Supprimer' ~ user.id) }}"
  />
  <button class="btn">Supprimer</button>
</form>
